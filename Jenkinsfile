pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Product build'
            }
        }
        stage('Code testing') {
            steps {
                echo 'Test Code'
            }
        }
        stage('Code Deploy') {
            steps {
                echo 'Deployed Code'
            }
        }
        stage('Release') {
            steps {
                echo 'Release success'
            }
        }
    }
}
